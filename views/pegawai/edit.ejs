<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <form class="" action="/pegawai/edit/<%=dataPegawai.id%>" method="post" enctype="multipart/form-data">
      <label>Nama Pegawai :</label>
      <input type="text" name="nama_pegawai" value="<%=dataPegawai.nama_pegawai%>">
      <label>Alamat :</label>
      <input type="text" name="alamat" value="<%=dataPegawai.alamat%>">
      <label>JK :</label>
      <input type="text" name="jk" value="<%=dataPegawai.jk%>">
      <label>TANGGAL LAHIR :</label>
      <input type="text" name="tgl_lahir" value="<%=dataPegawai.tgl_lahir%>">
      <label>TELP :</label>
      <input type="text" name="telp" value="<%=dataPegawai.telp%>">
      <label>STATUS PEGAWAI :</label>
      <input type="text" name="status_pegawai" value="<%=dataPegawai.status_pegawai%>">
      <label>FOTO :</label>
      <input type="file" name="foto" value="<%=dataPegawai.foto%>">
      <input type="submit" value="ADD">
    </form>
  </body>
</html>
