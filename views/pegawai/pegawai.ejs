<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table border="1px">
      <thead>
        <tr>
          <th>ID</th>
          <th>FOTO</th>
          <th>NAMA PEGAWAI</th>
          <th>ALAMAT</th>
          <th>JENIS KELAMIN</th>
          <th>TANGGAL LAHIR</th>
          <th>TELP</th>
          <th>STATUS</th>
          <th colspan="3">STATUS</th>
        </tr>
      </thead>
      <tbody>
        <% dataPegawai.forEach(rows => { %>
          <tr>
            <td><%= rows.id %></td>

            <td><img src="/images/<%= rows.foto %>" width="60px" height="50px"></td>
            <td><%= rows.nama_pegawai %></td>
            <td><%= rows.alamat %></td>
            <td><%= rows.jk %></td>
            <td><%= rows.tgl_lahir %></td>
            <td><%= rows.telp %></td>
            <td><%= rows.status_pegawai %></td>
            <td><a href="/pegawai/add">TAMBAH</a></td>
            <td><a href="/pegawai/edit/<%=rows.id%>">EDIT</a></td>
            <td><a href="/pegawai/delete/<%=rows.id%>">HAPUS</a></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
